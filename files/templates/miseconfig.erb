env_path = [
	# the absolute path of $XDG_DATA_HOME/pnpm
	"<%= @usr_home %>/.local/share/pnpm"
]

[tools]
awscli = "latest"
deno = "latest"
node = "22"
usage = "latest"

[env]
# the absolute path of $XDG_DATA_HOME/pnpm
PNPM_HOME = "<%= @usr_home %>/.local/share/pnpm"
 # the absolute path of $XDG_CONFIG_HOME/rtx/default-npm-package
RTX_NODE_DEFAULT_PACKAGES_FILE = "<%= @usr_home %>/.config/mise/default-npm-package"
